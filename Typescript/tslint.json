{
  "defaultSeverity": "error",
  "extends": [
    "tslint:all"
  ],
  "jsRules": {},
  "rules": { 
    "no-console": false,
    "no-empty": false,
    "typedef": false,
    "linebreak-style": false,
    "no-bitwise": false,
    "no-magic-numbers": false,
    "newline-per-chained-call": false,
    "no-require-imports": false
  },
  "rulesDirectory": []
}
